
<h2>Create new Expense</h2>
<%= form_for(:expenses, :url => {:action => 'create'}, html: {class: "form-horizontal"}) do |f| %>

  <!-- assumes same dir
  otherwise you'd put pages/subdir/form
  -->
  <%= render(:partial => "form", :locals => {:f => f}) %>

  <%= submit_tag "create expenses", :class => "btn btn-default" %>
<% end %>


